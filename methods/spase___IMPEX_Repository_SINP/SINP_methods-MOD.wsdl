            </xs:annotation>
          </xs:element>
          <xs:element type="xs:double" name="Rss" maxOccurs="1" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Distance to the magnetopause subsolar point</xs:documentation>
            </xs:annotation>
          </xs:element>          
          <xs:element type="ListOfDouble" name="IMF_B" maxOccurs="1" minOccurs="0"/>
        </xs:sequence>
      </xs:complexType>

      
      
