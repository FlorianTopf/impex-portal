      <xs:complexType name="extraParams_calculateDataPointValueFixedTime">
        <xs:annotation>
          <xs:documentation>List of individual parameters: SW Density, SW Velocity, IMF_B (Bx,By,Bz), Dst, AL, BD, BT, RD2, RD1, R2, R1.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element type="xs:double" name="SWDensity" maxOccurs="1" minOccurs="0"/>
          <xs:element type="xs:double" name="SWVelocity" maxOccurs="1" minOccurs="0"/>
          <xs:element type="ListOfDouble" name="IMF_B" maxOccurs="1" minOccurs="0"/>
          <xs:element type="xs:double" name="Dst" maxOccurs="1" minOccurs="0"/>
          <xs:element type="xs:double" name="AL" maxOccurs="1" minOccurs="0"/>
          <xs:element type="OutputFormat" name="OutputFileType" maxOccurs="1" minOccurs="0"/>
        </xs:sequence>
      </xs:complexType>

      
      
      <xs:complexType name="extraParams_calculateDataPointValue">
        <xs:annotation>
          <xs:documentation>Only OutputFileType for now.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element type="OutputFormat" maxOccurs="1" minOccurs="0" name="OutputFileType"/>
        </xs:sequence>
      </xs:complexType>
      
      
      
      <xs:complexType name="extraParams_calculateDataPointValueMercury">
        <xs:annotation>
          <xs:documentation> By default:'BD'=-196.0,'Flux'=131.0,'Rss'=1.35,'R2'=1.32,'DZ'=0.0,'IMF_B'=(0.0,0.0,0.0)</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element type="OutputFormat" name="OutputFileType" maxOccurs="1" minOccurs="0"/>
        <xs:element type="xs:double" name="BD" maxOccurs="1" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Dipole field strength on the equator of the Mercury</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element type="xs:double" name="Flux" maxOccurs="1" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Magnetic field strength on the inner edge of the tail current</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element type="xs:double" name="Rss" maxOccurs="1" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Sub-solar magnetopause distance in Mercury radii (2439km)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element type="xs:double" name="R2" maxOccurs="1" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Distance to the inner edge of tail current sheet</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element type="xs:double" name="DZ" maxOccurs="1" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Northward shift of the dipole relative to the center of the Mercury</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element type="ListOfDouble" name="IMF_B" maxOccurs="1" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Penetrated IMF components in HSM coord</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      
      
      
