  <wsdl:types>
    <xs:schema version="0.0" targetNamespace="http://smdc.sinp.msu.ru" 
      elementFormDefault="unqualified" xmlns="http://smdc.sinp.msu.ru" 
      xmlns:xs="http://www.w3.org/2001/XMLSchema">


      <!-- CHANGED TO OVERCOME TAVERNA PROBLEMS -->
      <xs:element type="xs:anyURI" name="ResourceID"/>
      <xs:element type="xs:string" name="Variable"/>
      <xs:element type="OutputFormat" name="OutputFileType"/>
      <xs:element type="xs:anyURI" name="url_XYZ">
        <xs:annotation>
          <xs:documentation>URL of the ASCII file containing the 3D coordinates. The coordinate system must be GSM.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element type="xs:dateTime" name="StartTime"/>
      <xs:element type="xs:dateTime" name="StopTime"/>
      <xs:element type="xs:duration" name="Sampling"/>
      <xs:element type="xs:string" name="Population"/>
      <xs:element type="FloatSequence" name="PlanePoint"/>
      <xs:element type="FloatSequence" name="PlaneNormalVector"/>
      <xs:element type="xs:double" name="Resolution"/>
      <xs:element type="xs:double" name="StepSize"/>
	  <xs:element type="xs:string" name="Spacecraft_name"/>
      <xs:element type="cube_size_array" name="cube_size_array"/>
      <xs:element type="Interpolation" name="InterpolationMethod"/>
      <xs:element type="ListOfDouble" name="ListOfDouble"/>
	  <!-- COPIED FROM THE SimDM -->
      <xs:simpleType name="FloatSequence">
        <xs:list itemType="xs:float"/>
      </xs:simpleType>
	
	  <xs:simpleType name="OutputFormat">
        <xs:annotation>
          <xs:documentation>
            two formats supported
          </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
          <xs:enumeration value="netCDF"/>
          <xs:enumeration value="VOTable"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType name="Interpolation">
        <xs:annotation>
          <xs:documentation>
            interpolation supported supported : right now only one
          </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
          <xs:enumeration value="linear"/>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType name="Population">
        <xs:annotation>
          <xs:documentation>List of population species from the selected NumericalOutput separated by a comma. Identified by the PopulationId. By default: all populations within the NumericalOutput are returned </xs:documentation>
        </xs:annotation>
        <xs:list itemType="xs:string"/>
      </xs:simpleType>
      <!-- MYSTERIOUSLY NOT WORKING WITH TAVERNA -->
      <!--xs:simpleType name="Variable">
        <xs:annotation>
