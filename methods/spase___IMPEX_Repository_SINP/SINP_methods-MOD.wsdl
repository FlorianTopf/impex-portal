        <xs:annotation>
          <xs:documentation>By default:'BDÐ¡'=3.0,'BT'=7.0,'RD2'=15.0,'RD1'=6.5,'Rss'=22.0,'R2'=18.0,'IMF_B'=(0.0,0.0,0.0)</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element type="OutputFormat" name="OutputFileType" maxOccurs="1" minOccurs="0"/>
          <xs:element type="xs:double" name="BDC" maxOccurs="1" minOccurs="0">
            <xs:annotation>
