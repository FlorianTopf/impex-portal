        </xs:annotation>
        <xs:list itemType="xs:string"/>
      </xs:simpleType-->

      <xs:complexType name="ListOfDouble">
        <xs:annotation>
          <xs:documentation>x,y,z</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element type="xs:double" maxOccurs="1" minOccurs="0" name="x"/>
