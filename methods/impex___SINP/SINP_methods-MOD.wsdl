      <xs:complexType name="extraParams_calculateDataPointValueFixedTime">
        <xs:annotation>
          <xs:documentation>List of individual parameters: SW Density, SW Velocity, IMF_B (Bx,By,Bz), Dst, AL, BD, BT, RD2, RD1, R2, R1.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element type="xs:double" name="SWDensity" maxOccurs="1" minOccurs="0"/>
          <xs:element type="xs:integer" name="SWVelocity" maxOccurs="1" minOccurs="0"/>
          <xs:element type="ListOfDouble" name="IMF_B" maxOccurs="1" minOccurs="0"/>
          <xs:element type="xs:double" name="Dst" maxOccurs="1" minOccurs="0"/>
          <xs:element type="xs:double" name="AL" maxOccurs="1" minOccurs="0"/>
          <xs:element type="xs:double" name="BD" maxOccurs="1" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Magnetic field at magnetodisc outer edge</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element type="xs:double" name="BT" maxOccurs="1" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Magnetic field at tail current inner edge</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element type="xs:double" name="RD2" maxOccurs="1" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Distance to the external edge of MD</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element type="xs:double" name="RD1" maxOccurs="1" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Distance to the inner edge of MD</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element type="xs:double" name="R1" maxOccurs="1" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Magnetopause stand-off distance</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element type="xs:double" name="R2" maxOccurs="1" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Distance to the inner edge of tail current sheet</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element type="OutputFormat" name="OutputFileType" maxOccurs="1" minOccurs="0"/>
        </xs:sequence>
      </xs:complexType>

      
      
